@using KnowYourTurf.Core
@using KnowYourTurf.Web
@using KnowYourTurf.Core.Html.FubuUI

@{
    Layout = "";
}
      
        @MvcHtmlString.Create(Html.HiddenFor(x => x.Vendor.EntityId, new { @class = "copyEntityId" }).ToString())
