@model KnowYourTurf.Web.Models.ListTypeViewModel
@using KnowYourTurf.Core
@using KnowYourTurf.Web
@using KnowYourTurf.Core.Html.FubuUI

@{
    Layout = "~/Views/Shared/_popupLayout.cshtml";
}
    <div id="errorMessagesPU" class="mf_ErrorMessages errorMessages"><ul></ul></div>
        @MvcHtmlString.Create(Html.HiddenFor(x => x.ListType.EntityId, new { @class = "copyEntityId" }).ToString())
            @MvcHtmlString.Create(Html.HiddenFor(x => x.AddEditUrl).ToString())
            <input type="hidden" id="EntityId" value="@Model.ListType.EntityId"/>
            @MvcHtmlString.Create(Html.HiddenFor(x => x.ListType.EntityId).ToString())
            @MvcHtmlString.Create(Html.ViewFor(x => x.ListType.Name).ToString())
            @MvcHtmlString.Create(Html.ViewFor(x => x.ListType.Description).ToString())
            @MvcHtmlString.Create(Html.ViewFor(x => x.ListType.Status).ToString())

