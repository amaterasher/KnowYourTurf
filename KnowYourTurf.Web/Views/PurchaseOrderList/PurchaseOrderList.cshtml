@model ListViewModel
@using KnowYourTurf.Core
@using KnowYourTurf.Web
@using HtmlTags

@{ Layout = "~/Views/Shared/_listLayout.cshtml"; }
@section HeaderScripts{
    @MvcHtmlString.Create(Script("kyt.PurchaseOrderController.js").ToString())
}
@section HeaderJavascript{

    $(document).ready(function () {
        new kyt.PurchaseOrderController(crudOptions);
    });
}

<div class="KYT_gridbreak">
@MvcHtmlString.Create(StyledButtonFor("addNew", WebLocalizationKeys.NEW_PURCHASE_ORDER).AddClass("KYT_float_left grid_3").ToString())
@MvcHtmlString.Create(StyledButtonFor("print", WebLocalizationKeys.PRINT).AddClass("KYT_float_right grid_1").ToString())
</div>
<table id="gridContainer" class="scroll"></table>
<div id="pager" class="scroll" style="text-align:center;"></div>
