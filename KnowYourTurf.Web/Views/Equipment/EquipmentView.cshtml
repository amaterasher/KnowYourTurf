@model KnowYourTurf.Web.Models.Equipment.EquipmentViewModel
@using FubuMVC.Core.Runtime
@using KnowYourTurf.Core.Html.FubuUI

@{
    Layout = "~/Views/Shared/_popupLayout.cshtml";

}
     
        @MvcHtmlString.Create(Html.HiddenFor(x => x.Equipment.EntityId, new { @class = "copyEntityId" }).ToString())
            @MvcHtmlString.Create(Html.HiddenFor(x => x.AddEditUrl).ToString())
            @MvcHtmlString.Create(Html.ViewFor(x => x.Equipment.Name).ToString())
            @MvcHtmlString.Create(Html.ViewFor(x => x.Equipment.Description).ToString())
            @MvcHtmlString.Create(Html.ViewFor(x => x.Equipment.Vendor).ToString())
            @MvcHtmlString.Create(Html.ViewFor(x => x.Equipment.TotalHours).ToString())

           @MvcHtmlString.Create(Html.ViewFor(x => x.Equipment.ImageUrl).HideLabel().AddClassToInput("KYT_displayImage").ToString())
