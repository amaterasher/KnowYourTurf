@model KnowYourTurf.Web.Models.OverseedRateNeededCalcViewModel
@using KnowYourTurf.Core.Html.Expressions;
@using KnowYourTurf.Core.Html.FubuUI
@using KnowYourTurf.Web.Controllers;

@{
    Layout = "~/Views/Shared/_FormLayout.cshtml";
}

    <div class="col left">
        <div class="fieldRow">
            @Html.ViewFor(x => x._calculatorDisplayName).ToHtmlTag()
        </div>
        <div class="fieldRow">
            @Html.SubmissionFor(x => x.FieldEntityId).ToHtmlTag()
        </div>
        <div class="fieldRow">
            @Html.SubmissionFor(x => x.ProductEntityId).ToHtmlTag()
        </div>
        <div class="fieldRow">
            @Html.SubmissionFor(x => x.BagsUsed).InputId("BagsUsed").ToHtmlTag()
        </div>
        <div class="fieldRow">
            @Html.SubmissionFor(x => x.OverSeedPercent).ToHtmlTag()
        </div>
        <div class="fieldRow">
            @Html.ViewFor(x => x.FieldArea).InputId("FieldArea").ToHtmlTag()
        </div>
        <div class="fieldRow">
            @Html.ViewFor(x => x.BagSize).InputId("BagSize").ToHtmlTag()
        </div>
        <div class="fieldRow">
            @Html.ViewFor(x => x.SeedRate).InputId("SeedRate").ToHtmlTag()
        </div>
    </div>
