@model WeatherViewModel
@using KnowYourTurf.Web
@using KnowYourTurf.Core.Html.FubuUI
@using KnowYourTurf.Web.Controllers

@{
    Layout = "~/Views/Shared/_popupLayout.cshtml";
}
       
        @MvcHtmlString.Create(Html.HiddenFor(x => x.Weather.EntityId, new { @class = "copyEntityId" }).ToString())
            @MvcHtmlString.Create(Html.HiddenFor(x => x.AddEditUrl).ToString())
            @MvcHtmlString.Create(Html.ViewFor(x => x.Weather.Date).ToString())
            @MvcHtmlString.Create(Html.ViewFor(x => x.Weather.DewPoint).ToString())
            @MvcHtmlString.Create(Html.ViewFor(x => x.Weather.HighTemperature).ToString())
            @MvcHtmlString.Create(Html.ViewFor(x => x.Weather.LowTemperature).ToString())
            @MvcHtmlString.Create(Html.ViewFor(x => x.Weather.RainPrecipitation).ToString())
            @MvcHtmlString.Create(Html.ViewFor(x => x.Weather.WindSpeed).ToString())


